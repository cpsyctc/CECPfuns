year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{nlme}: Linear and Nonlinear Mixed Effects Models",
         author = c(person("Evans", "Evans")),
         year = year,
         note = note,
         url = "https://github.com/cpsyctc/CECPfuns")
